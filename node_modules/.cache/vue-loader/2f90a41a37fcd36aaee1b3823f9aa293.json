                  <p class=\"card-description text-gray\">\n                        {{bio}}\n                    </p>\n                    <md-button class=\"md-raised md-primary github\"><a :href=\"github\">Github</a></md-button>\n                    <md-button class=\"md-raised md-primary twitter\"><a :href=\"twitter\">Twitter</a></md-button>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\">\n                    <h4>Name</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4>{{name}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\">\n                    <h4>DOB</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4>{{dob}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\" style=\"width: 160px\">\n                    <h4>Nationality</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4>{{nationality}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\">\n                    <h4>Race</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4>{{race}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\">\n                    <h4>Email</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4>{{email}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\" style=\"width: 150px\">\n                    <h4>Religion</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4>{{religion}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\">\n                    <h4>City</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4> {{city}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card class=\"profile-cards \">\n                <md-card-header class=\"card-header-special\" style=\"width: 180px\">\n                    <h4>Occupation</h4>\n                </md-card-header>\n                <md-card-content>\n                    <h4>{{occupation}}</h4>\n                </md-card-content>\n            </md-card>\n            <md-card style=\"margin-top: 35px\">\n                <md-card-header class=\"card-header-special\" style=\"background:#fff; color:#000; width: 180px;\">\n                    <h4>Fav Quote</h4>\n                </md-card-header>\n                <md-card-content v-for=\"(quote, index) in quotes\" :key=\"index\">\n                    <blockquote>\n                        <p>{{quote['text']}} <md-divider/> - {{quote['author']}}</p>\n                    </blockquote>\n                </md-card-content>\n            </md-card>\n        </div>\n        <div v-if=\"error\">\n            <md-card>\n                <md-card-content>\n                    <h3>Error: unable to load, <a style=\"color:black\" href=\"#!\" @click=\"loadData\">Retry</a> </h3>\n                </md-card-content>\n            </md-card>\n        </div>\n     </div>\n</template>\n\n<script>\n    import axios from \"axios\";\n    import url from \"@/url\"\n    export default {\n        name: 'skills',\n        data() {\n            return {\n                name: \"\",\n                dob: \"\",\n                nationality: \"\",\n                race: \"\",\n                email: \"\",\n                religion: \"\",\n                city: \"\",\n                occupation: \"\",\n                pic: \"\",\n                title: \"\",\n                bio: \"\",\n                github: \"\",\n                twitter: \"\",\n                quotes: [],\n                load: false,\n                error: false,\n            }\n        },\n        methods: {\n            loadData() {\n                this.data = []\n                this.error = false\n                axios.get(`${url}data/profile.json?timestamp=${new Date().getTime()}`).then(response => {\n                    let data =  response.data\n                    this.name = data.name\n                    this.dob = data.dob\n                    this.nationality = data.nationality\n                    this.race = data.race\n                    this.email = data.email\n                    this.religion = data.religion\n                    this.city = data.city\n                    this.occupation = data.occupation\n                    this.pic = data.pic\n                    this.title = data.title\n                    this.bio = data.bio\n                    this.github = data.github\n                    this.twitter = data.twitter\n                    this.quotes = data.quotes\n                    this.load = true\n                }).catch(err => {\n                    this.error = true\n                })\n            }\n        },\n        mounted() {\n            this.loadData()\n        }\n    }\n</script>"]}]}
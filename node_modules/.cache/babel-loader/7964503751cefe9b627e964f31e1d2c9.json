CA,wBAAA,KAAA,CAAA,QAAA,IAAA,KAAA,CAAA;AADA,SAAA;AAGA;AACA,KATA;AAUA,IAAA,SAVA,uBAUA;AACA,WAAA,EAAA,CAAA,MAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,IAAA;AACA,KAfA;AAgBA,IAAA,QAhBA,sBAgBA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA,CADA,CAGA;;AACA,MAAA,MAAA,CAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,QAAA,aAAA,KAAA,CAAA,IAAA,CAAA,SAAA,cAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,KAAA,CAAA,SAAA;AACA,OALA,EAKA,IALA;AAMA,KA1BA;AA2BA,IAAA,YA3BA,0BA2BA;AACA,WAAA,EAAA,CAAA,MAAA;;AAEA,UAAA,CAAA,KAAA,EAAA,CAAA,QAAA,EAAA;AACA,aAAA,QAAA;AACA;AACA;AAjCA;AA3BA,CAAA","sourcesContent":["<template>\n  <div>\n    <form novalidate class=\"md-layout\" @submit.prevent=\"validateData\">\n      <md-card class=\"md-layout-item md-size-50 md-small-size-100\">\n        <md-card-header>\n          <div class=\"md-title\">Contact me</div>\n        </md-card-header>\n\n        <md-card-content>\n          <div class=\"md-layout md-gutter\">\n            <div class=\"md-layout-item md-small-size-100\">\n              <md-field :class=\"getValidationClass('firstName')\">\n                <label for=\"name\">First Name</label>\n                <md-input name=\"name\" id=\"name\" autocomplete=\"given-name\" v-model=\"form.name\" :disabled=\"sending\" />\n                <span class=\"md-error\" v-if=\"!$v.form.firstName.required\">The name is required</span>\n                <span class=\"md-error\" v-else-if=\"!$v.form.firstName.minlength\">Invalid name</span>\n              </md-field>\n            </div>\n\n          <md-field :class=\"getValidationClass('email')\">\n            <label for=\"email\">Email</label>\n            <md-input type=\"email\" name=\"email\" id=\"email\" autocomplete=\"email\" v-model=\"form.email\" :disabled=\"sending\" />\n            <span class=\"md-error\" v-if=\"!$v.form.email.required\">The email is required</span>\n            <span class=\"md-error\" v-else-if=\"!$v.form.email.email\">Invalid email</span>\n          </md-field>\n\n        </md-card-content>\n\n        <md-progress-bar md-mode=\"indeterminate\" v-if=\"sending\" />\n\n        <md-card-actions>\n          <md-button type=\"submit\" class=\"md-primary\" :disabled=\"sending\">Send</md-button>\n        </md-card-actions>\n      </md-card>\n\n      <md-snackbar :md-active.sync=\"send\">The form submitted successfully!</md-snackbar>\n    </form>\n  </div>\n</template>\n\n<script>\n  import { validationMixin } from 'vuelidate'\n  import {\n    required,\n    email,\n    minLength,\n    maxLength\n  } from 'vuelidate/lib/validators'\n\n  export default {\n    name: 'Contact',\n    mixins: [validationMixin],\n    data: () => ({\n      form: {\n        name: null,\n        email: null,\n        comment: null\n      },\n      userSaved: false,\n      sending: false,\n      lastUser: null\n    }),\n    validations: {\n      form: {\n        name: {\n          required\n        },\n        comment: {\n          required\n        },\n        email: {\n          required,\n          email\n        }\n      }\n    },\n    methods: {\n      getValidationClass (fieldName) {\n        const field = this.$v.form[fieldName]\n\n        if (field) {\n          return {\n            'md-invalid': field.$invalid && field.$dirty\n          }\n        }\n      },\n      clearForm () {\n        this.$v.$reset()\n        this.form.name = null\n        this.form.email = null\n        this.form.comment = null\n      },\n      saveUser () {\n        this.sending = true\n\n        // Instead of this timeout, here you can call your API\n        window.setTimeout(() => {\n          this.lastUser = `${this.form.firstName} ${this.form.lastName}`\n          this.send = true\n          this.sending = false\n          this.clearForm()\n        }, 1500)\n      },\n      validateData () {\n        this.$v.$touch()\n\n        if (!this.$v.$invalid) {\n          this.saveUser()\n        }\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .md-progress-bar {\n    position: absolute;\n    top: 0;\n    right: 0;\n    left: 0;\n  }\n</style>"],"sourceRoot":"src/pages"}]}